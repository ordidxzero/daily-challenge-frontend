    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "sdkVersion": "38.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "android": {
      "package": "com.yujunheo.dailychallengefrontend"
    }
  }
}